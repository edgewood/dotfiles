#image/*; gimp --no-splash %s &
#image/*; display %s
image/*; evince-previewer %s
# Word 
application/msword; ~/bin/antiword %s; copiousoutput;
application/octet-stream; ~/bin/antiword %s; copiousoutput; test=echo %s|grep -Eqi "docx?$"
application/vnd.openxmlformats-officedocument.wordprocessingml.document; ~/bin/antiword %s; copiousoutput;
application/rtf; ~/bin/antiword %s; copiousoutput;
# PDF
application/pdf; pdfread %s
application/octet-stream; pdfread %s; test=echo %s|grep -qi "pdf$"
# Excel
application/vnd.ms-excel; antiexcel %s; copiousoutput;
application/octet-stream; antiexcel %s; copiousoutput; test=echo %s|grep -qi "xls$"
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; antiexcel %s; copiousoutput;
# HTML
text/html; links %s; nametemplate=%s.html
text/html; links -dump %s; nametemplate=%s.html; copiousoutput;
#text/html; w3m -T text/html %s; copiousoutput;
#text/html; lynx -dump %s; nametemplate=%s.html; copiousoutput;
# TNEF archives
application/ms-tnef;/usr/bin/tnef --list --file -; copiousoutput;
# ZIP archives
application/zip; /usr/bin/zipinfo -s -z %s; copiousoutput;
application/x-zip-compressed; /usr/bin/zipinfo -s -z %s; copiousoutput;
# TAR archives
application/x-tar-gz; mc -e %s
application/x-tar-gz; tar tzf %s; copiousoutput;
# gzip and bzip2
application/octet-stream; zcat %s; copiousoutput; test=echo %s|grep -qi "gz$"
application/octet-stream; bzcat %s; copiousoutput; test=echo %s|grep -Eqi "bz(ip)?2$"
application/x-gzip; zcat %s; copiousoutput;
application/x-bzip2; bzcat %s; copiousoutput;
